           function weatherInfo(city,flag) {
             var mflag=flag;

          //현재 날씨
            function request1(param) {
                  return new Promise(function(resolve, reject) {

          var xhr = new  XMLHttpRequest();
            url = 'http://api.openweathermap.org/data/2.5/weather'; /*URL 현재 날씨*/
            var queryParams;

            queryParams = '?' + encodeURIComponent('q') + '=' + encodeURIComponent(city) ; //지역
            queryParams += '&' + encodeURIComponent('appid') + '=' + appid+"&units=metric" //appid

            // console.log(url + queryParams);

         
                xhr.overrideMimeType("json");
                xhr.open('GET', url + queryParams, true);
                xhr.onreadystatechange = function () {

                  if (this.readyState == 4) {
                    if(this.status==200){
                      resolve(JSON.parse(xhr.responseText));
                     }
                      else {
                        reject('Error: ' + xhr.status);
                        alert('에러가 발생했습니다');
                    }

                  }

                };

                xhr.send('');
                

                  });
              }


              //예보 날씨
            function request2(param) {
                  return new Promise(function(resolve, reject) {

                    url2 = 'http://api.openweathermap.org/data/2.5/forecast'; //예보

                      var xhr = new XMLHttpRequest();
                      queryParams = '?' + encodeURIComponent('q') + '=' + encodeURIComponent(city); //지역
                      queryParams += '&' + encodeURIComponent('appid') + '=' + appid + "&units=metric"  //appid

                      // console.log(url2 + queryParams);

                      //예보 날씨
                      xhr.overrideMimeType("json");
                      xhr.open('GET', url2 + queryParams, true);
                      xhr.onreadystatechange = function () {

                        if (this.readyState == 4) {
                          if(this.status==200){
                            resolve(JSON.parse(xhr.responseText));
                            
                              // forequestWeather =  JSON.parse(this.responseText);
                          
                          }else {
                              reject('Error: ' + xhr.status);
                          }

                        }
                      };

                      xhr.send('');
                  });
              }

            //날씨 정보 받으면
              Promise.resolve()
              .then(request1)
              .catch(error => console.log(error) )
              .then(result=>{
                //현재 날씨 정보 받으면

  
                  //변수에 너어주기
                    if(mflag==1){

                      currentWeather=result;

                    }else if(mflag==2){

                        currentWeather2=result;
                    }else if(mflag==3){
                        currentWeather3=result;
                        
                    }   


                  //날씨 이미지가 이미지 리스트에 없는 경우 (undefined)
                  //프로미스에서 에러로 처리해 버림 -> 화면 표시안됨

                  // console.log(result);
       
                  changeCurrentInfo(currentWeather);  //메인화면에  정보를 붙이기 (현재 정보)
                  otherCountryInfo(currentWeather2,2); //other country 정보 붙이기
                  otherCountryInfo(currentWeather3,3);
                      

              })
              .then(request2)
              .catch(error => console.log(error))
              .then(result => {
                //예보 정보 받으면
                //변수에 너어주기

                  forequestWeather=result;


                  //정보 받으면 붙여주기
      
                  changeForcastInfo('day1',forequestWeather); //메인화면(아래)에  정보를 붙이기 (예보)
                  changeForcastInfo('day2',forequestWeather);
                  changeForcastInfo('day3',forequestWeather);
                  changeForcastInfo('day4',forequestWeather);
                  changeForcastInfo('day5',forequestWeather);


              });

          }

              function weImage(weather){

                // console.log(weather);

                var curentWeatherImage;
                var forImage = {
                  Rain: { imageName: 'wi wi-owm-504'},
                  Snow: { imageName: 'wi wi-owm-621'},
                  Smoke: { imageName: 'wi wi-owm-711'},
                  Dust: { imageName: 'wi wi-owm-761'},
                  Cloudy: { imageName: 'wi wi-owm-804'},
                  Hot: { imageName: 'wi wi-owm-904'},
                  Thunderstorm: { imageName: 'wi wi-owm-200'},
                  Lightning:{imageName: 'wi wi-owm-210:'},
                  Showers:{imageName: 'wi wi-owm-313'},
                  Windy:{imageName: 'wi wi-owm-905'},
                  Drizzle:{imageName: 'wi wi-owm-302'},
                  Sunny:{imageName: 'wi wi-day-sunny'},
                  Clear:{imageName: 'wi wi-day-sunny'},
                  Clouds: { imageName: 'wi wi-owm-804'},
                  Mist:{imageName: 'wi wi-owm-741'},
                  Haze:{imageName: 'wi wi-yahoo-20'}
                }

                if(forImage[weather]!=null){

                  if(forImage[weather].imageName!=null || typeof (forImage[weather].imageName)!= "undefined"){
                    curentWeatherImage=forImage[weather].imageName;
                    return curentWeatherImage;

                  }
                  // else if(forImage[weather].imageName==null || typeof (forImage[weather].imageName)== "undefined"){
                  //   return curentWeatherImage= '0';
                  // }
              }else{
               return  curentWeatherImage= '0';

              }

              }

              function otherCountryInfo(info,smallboxnum) {   //메인화면에  정보를 붙이기
                // console.log("otherCountryInfo" + info);

                // document.getElementById('mycity'+smallboxnum).innerHTML = info.name;
                document.getElementById('weaTemp'+smallboxnum).innerHTML = info.main.temp + "℃";
                document.getElementById('weaTempMax'+smallboxnum).innerHTML = info.main.temp_max + "℃";
                document.getElementById('weaTempMin'+smallboxnum).innerHTML = info.main.temp_min + "℃";
                document.getElementById('weaHu'+smallboxnum).innerHTML = info.main.humidity + "%";
                document.getElementById('weaDes'+smallboxnum).innerHTML = info.weather[0].description;
                document.getElementById('weaWin'+smallboxnum).innerHTML = info.wind.speed + "m/s";// 바람

                //  document.getElementById('weaCurrent').innerText = currentWeather.weather[0].main;
                
                var currentImage = "";
                var weather=info.weather[0].main;

                currentImage=weImage(weather); // 날씨에 맞는 이미지 있나 확인

                //  console.log(currentImage);

                if(typeof currentImage == "undefined" || currentImage == null || currentImage == ""||currentImage=='0'){
                  document.getElementById('weaCurrent'+smallboxnum).innerText = weather;

                }else{
                  document.getElementById('weaCurrent'+smallboxnum).children[0].className=currentImage;

                }
      
               }

               function changeCurrentInfo(info) {   //메인화면에  정보를 붙이기
