                //  document.getElementById('weaCurrent').innerText = currentWeather.weather[0].main;
                
                var currentImage = "";
                var weather=info.weather[0].main;

                currentImage=weImage(weather); // 날씨에 맞는 이미지 있나 확인

                  // console.log(currentImage);

                if(typeof currentImage == "undefined" || currentImage == null || currentImage == ""||currentImage=='0'){
                  document.getElementById('weaCurrent').innerText = weather;

                }else{
                  document.getElementById('weaCurrent').children[0].className=currentImage;

                }
      
               }


              function changeForcastInfo(f_day,info) { 
                // console.log("f_day :" +f_day +"  "+ "info"+info);
                var forcast = {
                  day1: { name: '1', list: 7, Image: "" },
                  day2: { name: '2', list: 15, Image: "" },
                  day3: { name: '3', list: 23, Image: "" },
                  day4: { name: '4', list: 31, Image: "" },
                  day5: { name: '5', list: 39, Image: "" }
                }
  
                document.getElementById('date'+forcast[f_day].name).innerHTML = (info['list'][forcast[f_day].list]['dt_txt']).substring(0,10);


                forcastImage=forcast[f_day].Image;
                var forcastWeather= info['list'][forcast[f_day].list]['weather'][0]['main'];

                forcastImage=weImage(forcastWeather); // 날씨에 맞는 이미지 있나 확인


               if(typeof forcastImage == "undefined" || forcastImage == null || forcastImage == "" || forcastImage == '0'){ // forcastImage undefined null ""이면  이름으로

                  document.getElementById('weaCast'+forcast[f_day].name).innerText =forcastWeather;

                }else{// forcastImage undefined null ""이 아니면  이미지로

                  document.getElementById('weaCast'+forcast[f_day].name).children[0].className = forcastImage;
                }

                


                if (info['list'][forcast[f_day].list]['weather'][0]['main'] == 'Rain') {
                  forcastImage = 'wi wi-owm-504';
                } else if (info['list'][forcast[f_day].list]['weather'][0]['main'] == 'Drizzle') {

                  forcastImage = 'wi wi-owm-504';
                } 
                
                if(typeof forcastImage == "undefined" || forcastImage == null || forcastImage == "" || forcastImage == '0'){ // forcastImage undefined null ""이면  이름으로

                  document.getElementById('weaCast'+forcast[f_day].name).innerText = info['list'][forcast[f_day].list]['weather'][0]['main'];

                }else{// forcastImage undefined null ""이 아니면  이미지로

                  document.getElementById('weaCast'+forcast[f_day].name).children[0].className = forcastImage;
                }

                

              }


     
         weatherInfo(cityname,1); //cityname으로  정보 가져옴
         weatherInfo('Busan',2); //cityname으로  정보 가져옴 
        weatherInfo('Sydney',3); //cityname으로  정보 가져옴
